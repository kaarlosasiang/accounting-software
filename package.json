{
  "name": "accounting-system",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/ensure-better-auth-tsconfig-base.mjs",
    "dev": "pnpm --parallel --filter \"./apps/*\" dev",
    "dev:api": "pnpm --filter api dev",
    "dev:app": "pnpm --filter frontend dev",
    "dev:site": "pnpm --filter site dev",
    "storybook": "pnpm --filter ui storybook",
    "build": "pnpm --recursive build",
    "build:api": "pnpm --filter api build",
    "build:app": "pnpm --filter frontend build",
    "build:site": "pnpm --filter site build",
    "build:validators": "pnpm --filter @sas/validators build",
    "build:ui": "pnpm --filter @sas/ui build",
    "start:api": "pnpm --filter api start",
    "start:frontend": "pnpm --filter frontend start",
    "preview:site": "pnpm --filter site preview",
    "docker:build:api": "docker build -f apps/backend/Dockerfile -t sas-api:local .",
    "lint": "pnpm --recursive exec eslint",
    "clean": "pnpm --recursive exec rm -rf dist node_modules",
    "install:all": "pnpm install",
    "typecheck": "pnpm --recursive exec tsc --noEmit",
    "validators:watch": "pnpm --filter @sas/validators dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.0",
  "pnpm": {
    "overrides": {
      "react": "19.2.4",
      "react-dom": "19.2.4",
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.3"
    }
  },
  "devDependencies": {
    "baseline-browser-mapping": "^2.9.11"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2"
  }
}
